.u-node {
  width: 20rpx;
  height: 20rpx;
  border-radius: 50%;
  display: flex;
  justify-content: center;
  align-items: center;
  background: #FF4B3C;
}
.u-order-title {
  color: #FF4B3C;
  font-weight: bold;
  font-size: 32rpx;
}
.u-order-desc {
  color: #FF4B3C;
  font-size: 28rpx;
  margin-bottom: 6rpx;
}
.u-order-time {
  color: #FF4B3C;
  font-size: 26rpx;
}
.time {
  color: #999999;
  margin-bottom: 20rpx;
}
.toutiao_item {
  background-color: #f5f5f5;
  padding: 20rpx;
  width: 600rpx;
  border-radius: 20rpx;
  /* margin-left: 20rpx; */
}
.aass {
  height: 50rpx;
  line-height: 50rpx;
}
